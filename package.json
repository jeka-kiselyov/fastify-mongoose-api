  "author": "Jeka Kiselyov",
  "license": "MIT",
  "dependencies": {
    "@fastify/formbody": "^7.4.0",
    "debug": "^4.3.4",
    "fastify": "^4.26.0",
    "fastify-plugin": "^4.5.1",
    "mongoose": "^8.1.1"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "grunt": "^1.6.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-env": "^1.0.1",
    "grunt-eslint": "^24.3.0",
    "grunt-tape": "^0.1.0",
    "prettier": "^3.2.5",
    "tap": "^18.7.0"
  }

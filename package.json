  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^16.0.0",
    "grunt": "^1.6.1",
    "grunt-contrib-watch": "^1.1.0",
